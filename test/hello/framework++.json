{
    "name": "top",
    "env" : {
        "appProtocol" : "http",
        "appPublisher" :"root",
        "appLocalName" : "graphql",
        "appSuffix" : "vcap.me:3000"
    },
    "components": [
        {
            "name": "log",
            "env" : {
                  "logLevel" : "DEBUG"

            }

        },
        {
            "name": "security",
            "module": null
        },
        {
            "name": "cp2"
        },
        {
            "name": "graphql",
            "module": "../../index.js#plug",
            "description": "GraphQL service.",
            "env" : {
                "schemaDir": "process.env.SCHEMA_DIR||null",
                "schemaFileName" : "process.env.SCHEMA_FILE_NAME||ca.graphql"
            }
        }
    ]
}
